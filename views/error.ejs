<% extend('./layouts/layout') %>

<div class="my-4 row justify-content-center">
  <div class="col-md-9">
    <h1 class="text-primary pb-2"><%- massage %></h1>
    <% var error = error || {} %>
    <% if(error){ %>
      <p><%= error.status %></p>
      <pre><%= error.stack %></pre>
    <% } %>
    <a href="/">回到首頁</a>
  </div>
</div>